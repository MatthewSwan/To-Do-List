<h1> Welcome to Aarons List </h1>
<% @list.each do |item|  %>
  <div>
    <%= link_to item.item, list_path(item) %>
</div>
<% end %>

<p></p>
<%= form_tag("/list", method: "post") do %>
  <%= label_tag(:item, "enter a new list item") %>
  <%= text_field_tag(:item) %>
  <%= submit_tag("Submit") %>
<% end %>
